{
  "name": "detox-examples",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=10.12.0"
  },
  "author": "Tal David Shitrit",
  "scripts": {
    "ios": "react-native run-ios --scheme=myATT",
    "android": "react-native run-android --variant=debug  --appFolder=MyAttNative --appId com.att.myWirelessTest --main-activity ui.SplashScreenActivity --tasks installDevPackageDebug",
    "preios": "npm run patch-xcode-12 && if [ ! -e ios ]; then ln -s ../ios/myatt-ios ios \n fi && cd ios && pod install && cd ..",
    "preandroid": "if [ ! -e android ]; then ln -s ../android/myatt-thin-native-android android \n fi",
    "clean-android": "cd android && ./gradlew clean",
    "clean-ios": "cd ios && xcodebuild clean",
    "husky:fix": "node ./patches/huskyfix.js",
    "start": "react-native start",
    "start:reset": "npm run start -- --reset-cache",
    "start:dev": "concurrently -r 'npm run compile:dev' 'npm run start'",
    "start:detox": "export DETOX_TEST=1 && react-native start",
    "test": "jest --config jest.config.js",
    "test:updateSnapshot": "jest --updateSnapshot",
    "test:dev": "jest --config jest.config.js --watchAll",
    "test:cov": "jest --config jest.config.js --coverage --u",
    "test:logResults": "jest --config jest.config.js --u --json --outputFile=.DevOps/unitTestsResult.json",
    "compile": "tsc -p . --pretty",
    "compile:dev": "npm run compile -- -w",
    "clean": "tsc --build --clean",
    "bundle-android-thin": "react-native --verbose bundle --platform android --dev false --entry-file index.js --bundle-output ../android/myatt-thin-native-android/myAttNative/src/main/assets/index.android.bundle --assets-dest ../android/myatt-thin-native-android/myAttNative/src/main/res",
    "detox-run-ist:ios": "export TEST_TYPE=IST && npm run detox-run:ios",
    "docs": "rimraf ./docs/internal/rn/ && typedoc ./src/index.tsx && node ./docs/docs.js ./rn",
    "docs:serve": "docsify serve ./docs/internal",
    "android-emulator:install-package": "${ANDROID_HOME}/tools/bin/sdkmanager --install 'system-images;android-29;default;x86_64'",
    "android-emulator:create-avd": "rm -f ${HOME}/.android/avd/${emulatorName}.avd/*.lock && ${ANDROID_HOME}/tools/bin/avdmanager --verbose create avd --force --name 'Pixel_4_API_30' --package 'system-images;android-29;default;x86_64' -d 'pixel_xl'",
    "android-emulator:start": "${ANDROID_HOME}/emulator/emulator -avd Pixel_4_API_30 -cores 2 -gpu auto -accel on -memory 1536 -noaudio  -no-boot-anim -wipe-data -no-snapshot -partition-size 1536",
    "detox-build:android": "export DETOX_TEST=1 && detox build --configuration android",
    "detox-run:android": "sed -i bak 's/Platform.isTesting/false/g' node_modules/react-native/Libraries/Animated/src/Animated.js && export DETOX_TEST=1 && detox test --configuration android --maxWorkers 1 --retries 1 --record-logs failing --take-screenshots failing",
    "detox-build:ios": "export DETOX_TEST=1 && export FORCE_BUNDLING=true && detox build --configuration ios:jenkins",
    "detox-build:ios:debug": "export DETOX_TEST=1 && export FORCE_BUNDLING=true && detox build --configuration ios:debug",
    "detox-run:ios": "detox test --configuration ios:jenkins --maxWorkers 3 --retries 1 --record-logs failing --take-screenshots failing"
  },
